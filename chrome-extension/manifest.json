{
  "name": "ScriptObservatory",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Keep track of what scripts you run.",
  "default_locale": "en",
  "permissions": [
    "http://*/*", "https://*/*", "webRequest", "webRequestBlocking", "tabs"
  ],
  "background": {
    "scripts": [
      "src/background.js",
      "src/sha256.js"
    ],
    "persistent": true
  }
}
